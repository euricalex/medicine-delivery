{
  "name": "medicine-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://euricalex.github.io/medicine-delivery/",
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "formik": "^2.4.5",
    "mongodb": "^6.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "sass": "^1.71.1",
    "web-vitals": "^2.1.4",
    "yup": "^1.4.0"
  },
  "scripts": {
    "start": "npm-run-all --parallel start:server start:client",
    "start:server": "cross-env NODE_ENV=production node ./server.js",
    "start:client": "react-scripts start",
    "start:dev": "npm-run-all --parallel start:server:dev start:client:dev",
    "start:server:dev": "cross-env NODE_ENV=development nodemon ./server.js",
    "start:client:dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "gh-pages": "^6.1.1",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5"
  }
}
